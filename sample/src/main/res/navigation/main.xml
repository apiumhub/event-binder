<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/list">
    <fragment
        android:id="@+id/list"
        android:name="dev.martori.events.sample.ui.MainListFragment"
        tools:layout="@layout/fragment_main_list"
        android:label="MainListFragment">
        <action
            android:id="@+id/action_list_to_details"
            app:destination="@id/details" />
    </fragment>
    <fragment
        android:id="@+id/details"
        android:name="dev.martori.events.sample.ui.DetailsFragment"
        tools:layout="@layout/fragment_details"
        android:label="DetailsFragment">
        <argument
            android:name="id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/libTestFragment"
        android:name="dev.martori.events.sample.ui.LibTestFragment"
        android:label="fragment_lib_test"
        tools:layout="@layout/fragment_lib_test" />
</navigation>